{
  "name": "react-native-swift-registry",
  "version": "0.0.1",
  "peerDependencies": {
    "react-native-swift": "^1.0.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/rhdeck/react-native-swift-registry#README",
  "description": "Adds a registry of data usable from native components",
  "scripts": {
    "bridge": "react-native-swift-bridge",
    "watch": "react-native-swift-bridge --watch",
    "addpod": "react-native-pod addpod",
    "removepod": "react-native-pod removepod"
  },
  "main": "RNSwiftBridge.js",
  "isSwift": true,
  "devDependencies": {
    "react-native-swift-bridge": "^2.1.0"
  },
  "rnpm": {
    "commands": {
      "prelink": "node node_modules/.bin/react-native-swift-registry"
    }
  },
  "bin": {
    "react-native-swift-registry": "prelink.js"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "plist": "^3.0.1"
  }
}
